<?xml version="1.0" encoding="utf-8"?>
<selector  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <item android:state_checked="true">
        <shape android:shape="ring">
            <corners android:topLeftRadius="5dp" android:bottomLeftRadius="5dp" />
            <stroke android:width="1dp" android:color="@color/theme_thrid_gray" />
            <solid android:color="@color/theme_thrid_gray" />
            <padding android:left="@dimen/activity_padding_m" android:top="2dp" android:right="@dimen/activity_padding_m" android:bottom="2dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="ring">
            <corners android:topLeftRadius="5dp" android:bottomLeftRadius="5dp" />
            <stroke android:width="1dp" android:color="@color/record_white_transparency33" />
            <solid android:color="@color/normal_primary_color" />
            <padding android:left="@dimen/activity_padding_m" android:top="2dp" android:right="@dimen/activity_padding_m" android:bottom="2dp" />
        </shape>
    </item>
</selector>
